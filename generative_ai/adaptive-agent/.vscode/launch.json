    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agent/dev.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "python": "${workspaceFolder}/agent/.venv/bin/python",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/agent",
        }
    ]
}
